    <mapping class="com.ardor3d.extension.model.collada.binding.fx.DaeNewparam" name="newparam" post-set="postset">
        <value name="sid" set-method="setSid" get-method="getSid" style="attribute" usage="optional"/>
        <structure name="surface" field="surface" usage="optional"/>
        <structure field="sampler" usage="optional"/>
    </mapping>

	<mapping class="com.ardor3d.extension.model.collada.binding.fx.DaeSurface" abstract="true" post-set="postsetSpecifiedInAbstract">
        <value name="type" set-method="setTypeFromCollada" get-method="getTypeAsCollada" style="attribute" usage="optional"/>
		<collection field="initFroms" usage="optional" pre-set="preset">
			<structure name="init_from" type="com.ardor3d.extension.model.collada.binding.fx.DaeInitFrom" usage="optional"/>
		</collection>
        <structure name="format" usage="optional"/>
	</mapping>

	<mapping class="com.ardor3d.extension.model.collada.binding.fx.DaeInitFrom" abstract="true" post-set="postsetSpecifiedInAbstract">
		<value name="mip" field="mip" style="attribute" usage="optional"/>
		<value name="slice" field="slice" style="attribute" usage="optional"/>
		<value name="face" field="face" style="attribute" usage="optional"/>
		<value field="value" style="text"/>
	</mapping>

	<mapping class="com.ardor3d.extension.model.collada.binding.fx.DaeSampler" abstract="true" post-set="postsetSpecifiedInAbstract">
		<value name="source" field="source"/>
		<value name="wrap_s" field="wrapS" usage="optional"/>
		<value name="minfilter" field="minfilter" usage="optional"/>
		<value name="magfilter" field="magfilter" usage="optional"/>
		<structure name="mipfilter" usage="optional"/>
		<structure name="mipmap_maxlevel" usage="optional"/>
		<structure name="mipmap_bias" usage="optional"/>
		<structure name="extra" usage="optional"/>
	</mapping>

	<mapping class="com.ardor3d.extension.model.collada.binding.fx.DaeSampler2D" name="sampler2D" post-set="postset" 
			ordered="false" extends="com.ardor3d.extension.model.collada.binding.fx.DaeSampler">
		<structure map-as="com.ardor3d.extension.model.collada.binding.fx.DaeSampler"/>
		<value name="wrap_t" field="wrapT" usage="optional"/>
		<structure name="border_color" field="borderColor" usage="optional"/>
	</mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.fx.DaeTechnique" abstract="true" post-set="postsetSpecifiedInAbstract">
        <value name="id" set-method="setId" get-method="getId" style="attribute" usage="optional"/>
        <value name="sid" set-method="setSid" get-method="getSid" style="attribute" usage="optional"/>
        <value name="name" set-method="setName" get-method="getName" style="attribute" usage="optional"/>
        <structure name="blinn" field="blinn" type="com.ardor3d.extension.model.collada.binding.fx.DaeBlinnPhong" usage="optional"/>
        <structure name="phong" field="phong" type="com.ardor3d.extension.model.collada.binding.fx.DaeBlinnPhong" usage="optional"/>
        <structure name="lambert" field="lambert" type="com.ardor3d.extension.model.collada.binding.fx.DaeBlinnPhong" usage="optional"/>
        <structure name="extra" usage="optional"/>
    </mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.fx.DaeBlinnPhong" abstract="true" post-set="postsetSpecifiedInAbstract">
        <value name="id" set-method="setId" get-method="getId" style="attribute" usage="optional"/>
        <value name="sid" set-method="setSid" get-method="getSid" style="attribute" usage="optional"/>
        <value name="name" set-method="setName" get-method="getName" style="attribute" usage="optional"/>
        <structure name="emission" usage="optional">
            <structure field="emissionColor" name="color" usage="optional" type="com.ardor3d.extension.model.collada.binding.core.DaeColor"/>
            <structure field="emissionTexture" name="texture" usage="optional" type="com.ardor3d.extension.model.collada.binding.fx.DaeTexture"/>
        </structure>
        <structure name="ambient" usage="optional">
            <structure field="ambientColor" name="color" usage="optional" type="com.ardor3d.extension.model.collada.binding.core.DaeColor"/>
            <structure field="ambientTexture" name="texture" usage="optional" type="com.ardor3d.extension.model.collada.binding.fx.DaeTexture"/>
        </structure>
        <structure name="diffuse" usage="optional">
            <structure field="diffuseColor" name="color" usage="optional" type="com.ardor3d.extension.model.collada.binding.core.DaeColor"/>
            <structure field="diffuseTexture" name="texture" usage="optional" type="com.ardor3d.extension.model.collada.binding.fx.DaeTexture"/>
        </structure>
        <structure name="specular" usage="optional">
            <structure field="specularColor" name="color" usage="optional" type="com.ardor3d.extension.model.collada.binding.core.DaeColor"/>
            <structure field="specularTexture" name="texture" usage="optional" type="com.ardor3d.extension.model.collada.binding.fx.DaeTexture"/>
        </structure>

        <structure name="shininess" field="shininess" type="com.ardor3d.extension.model.collada.binding.core.DaeFloat" usage="optional"/>

        <structure name="reflective" usage="optional">
            <structure field="reflectiveColor" name="color" usage="optional" type="com.ardor3d.extension.model.collada.binding.core.DaeColor"/>
            <structure field="reflectiveTexture" name="texture" usage="optional" type="com.ardor3d.extension.model.collada.binding.fx.DaeTexture"/>
        </structure>
        <structure name="reflectivity" field="reflectivity" type="com.ardor3d.extension.model.collada.binding.core.DaeFloat" usage="optional"/>

        <structure name="transparent" usage="optional">
            <structure field="transparentColor" name="color" usage="optional" type="com.ardor3d.extension.model.collada.binding.core.DaeColor"/>
            <structure field="transparentTexture" name="texture" usage="optional" type="com.ardor3d.extension.model.collada.binding.fx.DaeTexture"/>
        </structure>
        <structure name="transparency" field="transparency" type="com.ardor3d.extension.model.collada.binding.core.DaeFloat" usage="optional"/>
        <structure name="index_of_refraction" field="ioRefraction" type="com.ardor3d.extension.model.collada.binding.core.DaeFloat" usage="optional"/>
    </mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.core.DaeAsset" name="asset" post-set="postset">
        <structure name="contributor" usage="optional"/>
        <value name="created" field="created" usage="optional"/>
        <structure name="keywords" usage="optional"/>
        <value name="modified" field="modified" usage="optional"/>
        <value name="revision" field="revision" usage="optional"/>
        <structure name="subject" usage="optional"/>
        <structure name="title" usage="optional"/>
        <structure name="unit" usage="optional"/>
        <structure name="up_axis" usage="optional"/>
    </mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.core.DaeFloat" abstract="true" post-set="postsetSpecifiedInAbstract">
        <value name="float" set-method="setFloatValue" get-method="getFloatValue"/>
    </mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.core.DaeColor" abstract="true" post-set="postsetSpecifiedInAbstract">
        <value style="text" set-method="setColor" get-method="getColor"/>
    </mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.fx.DaeTexture" abstract="true" post-set="postsetSpecifiedInAbstract">
        <value name="texture" field="texture" style="attribute"/>
        <value name="texcoord" field="texcoord" style="attribute"/>
        <structure name="extra" usage="optional"/>
    </mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.core.DaeGeometry" name="geometry" pre-set="postset">
        <value name="id" set-method="setId" get-method="getId" style="attribute" usage="optional"/>
        <value name="sid" set-method="setSid" get-method="getSid" style="attribute" usage="optional"/>
        <value name="name" set-method="setName" get-method="getName" style="attribute" usage="optional"/>
        <structure name="mesh" field="mesh" usage="optional" type="com.ardor3d.extension.model.collada.binding.core.DaeMesh"/>
        <structure name="extra" usage="optional"/>
    </mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.core.DaeMesh" abstract="true" pre-set="postsetSpecifiedInAbstract">
        <collection field="sourceData" item-type="com.ardor3d.extension.model.collada.binding.core.DaeSource" pre-set="preset"/>
        <structure name="vertices" field="vertices" type="com.ardor3d.extension.model.collada.binding.core.DaeVertices"/>

		<structure ordered="false" usage="optional">
		    <collection field="lines" usage="optional" pre-set="preset">
		        <structure name="lines" type="com.ardor3d.extension.model.collada.binding.core.DaeLines" usage="optional"/>
		    </collection>
		    <collection field="linestrips" usage="optional" pre-set="preset">
		        <structure name="linestrips" type="com.ardor3d.extension.model.collada.binding.core.DaeLinestrips" usage="optional"/>
		    </collection>
		    <collection field="polygons" usage="optional" pre-set="preset">
		        <structure name="polygons" type="com.ardor3d.extension.model.collada.binding.core.DaePolygons" usage="optional"/>
		    </collection>
		    <collection field="polylist" usage="optional" pre-set="preset">
		        <structure name="polylist" type="com.ardor3d.extension.model.collada.binding.core.DaePolylist" usage="optional"/>
		    </collection>
		    <collection field="triangles" usage="optional" pre-set="preset">
		        <structure name="triangles" type="com.ardor3d.extension.model.collada.binding.core.DaeTriangles" usage="optional"/>
		    </collection>
		    <collection field="trifans" usage="optional" pre-set="preset">
		        <structure name="trifans" type="com.ardor3d.extension.model.collada.binding.core.DaeTrifans" usage="optional"/>
		    </collection>
		    <collection field="tristrips" usage="optional" pre-set="preset">
		        <structure name="tristrips" type="com.ardor3d.extension.model.collada.binding.core.DaeTristrips" usage="optional"/>
		    </collection>
		</structure>
    </mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.core.DaeSource" name="source" post-set="postset">
        <value name="id" set-method="setId" get-method="getId" style="attribute" usage="optional"/>
        <value name="sid" set-method="setSid" get-method="getSid" style="attribute" usage="optional"/>
        <value name="name" set-method="setName" get-method="getName" style="attribute" usage="optional"/>
        <structure name="float_array" field="floatArray" usage="optional" type="com.ardor3d.extension.model.collada.binding.core.DaeFloatArray"/>
        <structure name="int_array" field="intArray" usage="optional" type="com.ardor3d.extension.model.collada.binding.core.DaeIntegerArray"/>
        <structure name="technique_common">
            <structure name="accessor" field="commonAccessor" type="com.ardor3d.extension.model.collada.binding.core.DaeAccessor"/>
        </structure>
    </mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.core.DaeFloatArray" abstract="true" pre-set="postsetSpecifiedInAbstract"
