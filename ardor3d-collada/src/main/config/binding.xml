<binding xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="binding.Daexsd"
         track-source="true">
    <namespace uri="http://www.collada.org/2005/11/COLLADASchema" default="elements"/>


    <mapping class="com.ardor3d.extension.model.collada.binding.core.Collada" name="COLLADA" post-set="postset">
        <structure field="asset"/>
        <structure ordered="false">
	        <collection field="libraryEffects" usage="optional" pre-set="preset">
	            <structure name="library_effects" type="com.ardor3d.extension.model.collada.binding.DaeLibraries"/>
	        </collection>
	        <collection field="libraryMaterials" usage="optional" pre-set="preset">
	            <structure name="library_materials" type="com.ardor3d.extension.model.collada.binding.DaeLibraries"/>
	        </collection>
	        <collection field="libraryGeometries" usage="optional" pre-set="preset">
	            <structure name="library_geometries" type="com.ardor3d.extension.model.collada.binding.DaeLibraries"/>
	        </collection>
	        <collection field="libraryVisualScenes" usage="optional" pre-set="preset">
	            <structure name="library_visual_scenes" type="com.ardor3d.extension.model.collada.binding.DaeLibraries"/>
	        </collection>
	        <collection field="libraryNodes" usage="optional" pre-set="preset">
	            <structure name="library_nodes" type="com.ardor3d.extension.model.collada.binding.DaeLibraries"/>
	        </collection>
	        <collection field="libraryImages" usage="optional" pre-set="preset">
	            <structure name="library_images" type="com.ardor3d.extension.model.collada.binding.DaeLibraries"/>
	        </collection>
	        
	        <!-- TODO: -->
	        <structure name="library_animations" usage="optional"/>
	        <structure name="library_animation_clips" usage="optional"/>
	        <structure name="library_cameras" usage="optional"/>
	        <structure name="library_controllers" usage="optional"/>
	        <structure name="library_force_fields" usage="optional"/>
	        <structure name="library_lights" usage="optional"/>
	        <structure name="library_physics_materials" usage="optional"/>
	        <structure name="library_physics_models" usage="optional"/>
	        <structure name="library_physics_scenes" usage="optional"/>
        </structure>

        <structure name="scene" field="scene" type="com.ardor3d.extension.model.collada.binding.core.DaeScene" usage="optional"/>
        <structure name="extra" usage="optional"/>
    </mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.DaeLibraries" abstract="true" pre-set="postsetSpecifiedInAbstract">
        <structure field="asset" usage="optional"/>
        <collection field="libraries" pre-set="preset" post-set="postset"/>
    </mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.fx.DaeEffect" name="effect" post-set="postset">
        <value name="id" set-method="setId" get-method="getId" style="attribute" usage="optional"/>
        <value name="name" set-method="setName" get-method="getName" style="attribute" usage="optional"/>
        <structure name="asset" usage="optional"/>
        <structure name="profile_COMMON" field="profileCommon" type="com.ardor3d.extension.model.collada.binding.fx.DaeProfileCommon"/>
        <structure name="extra" usage="optional"/>
    </mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.fx.DaeImage" name="image" post-set="postset">
        <value name="id" set-method="setId" get-method="getId" style="attribute" usage="optional"/>
        <value name="name" set-method="setName" get-method="getName" style="attribute" usage="optional"/>
        <value name="format" field="format" style="attribute" usage="optional"/>
        <value name="height" field="height" style="attribute" usage="optional"/>
        <value name="width" field="width" style="attribute" usage="optional"/>
        <value name="depth" field="depth" style="attribute" usage="optional"/>
        <structure name="asset" usage="optional"/>
        <value name="data" field="data" usage="optional"/>
        <value name="init_from" field="initFrom" usage="optional"/>
        <structure name="extra" usage="optional"/>
    </mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.fx.DaeMaterial" name="material" post-set="postset">
        <value name="id" set-method="setId" get-method="getId" style="attribute" usage="optional"/>
        <value name="name" set-method="setName" get-method="getName" style="attribute" usage="optional"/>
        <structure name="asset" usage="optional"/>
        <structure name="instance_effect" field="instanceEffect" type="com.ardor3d.extension.model.collada.binding.fx.DaeInstanceEffect"/>
        <structure name="extra" usage="optional"/>
    </mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.fx.DaeInstanceEffect" abstract="true" post-set="postsetSpecifiedInAbstract">
        <value name="sid" set-method="setSid" get-method="getSid" style="attribute" usage="optional"/>
        <value name="name" set-method="setName" get-method="getName" style="attribute" usage="optional"/>
        <value name="url" field="url" style="attribute"/>
    </mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.fx.DaeProfileCommon" abstract="true" post-set="postsetSpecifiedInAbstract">
        <value name="id" set-method="setId" get-method="getId" style="attribute" usage="optional"/>
        <structure name="asset" usage="optional"/>
        <collection field="newparams" item-type="com.ardor3d.extension.model.collada.binding.fx.DaeNewparam" pre-set="preset"/>
        <structure name="technique" field="technique" type="com.ardor3d.extension.model.collada.binding.fx.DaeTechnique"/>
        <structure name="extra" usage="optional"/>
        <!--  <collection field="extras" item-type="com.ardor3d.extension.model.collada.binding.core.DaeExtra" pre-set="preset"/>-->
    </mapping>

