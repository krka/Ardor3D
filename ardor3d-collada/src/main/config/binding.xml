        <value name="name" set-method="setName" get-method="getName" style="attribute" usage="optional"/>
        <collection field="inputs">
            <structure name="input" type="com.ardor3d.extension.model.collada.binding.core.DaeInputUnshared"/>
        </collection>
    </mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.core.DaeInputUnshared" abstract="true" pre-set="postsetSpecifiedInAbstract">
        <value name="source" field="source" style="attribute"/>
        <value name="semantic" field="semantic" style="attribute"/>
    </mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.core.DaeInputShared" abstract="true" pre-set="postsetSpecifiedInAbstract">
        <value name="offset" field="offset" style="attribute"/>
        <value name="source" field="source" style="attribute"/>
        <value name="semantic" field="semantic" style="attribute"/>
        <value name="set" field="set" style="attribute" usage="optional"/>
    </mapping>

