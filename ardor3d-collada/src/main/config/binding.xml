        <value name="sid" set-method="setSid" get-method="getSid" style="attribute" usage="optional"/>
        <value name="name" set-method="setName" get-method="getName" style="attribute" usage="optional"/>
        <value name="type" set-method="setTypeText" get-method="getTypeText" style="attribute"/>
    </mapping>

    <mapping class="com.ardor3d.extension.model.collada.binding.core.DaeVertices" abstract="true" post-set="postsetSpecifiedInAbstract">
        <value name="id" set-method="setId" get-method="getId" style="attribute" usage="optional"/>
